{
  "manifest_version": 2,

  "name": "Page Latency Stats",
  "description": "Find Page Load Latency Statistics",
  "version": "1.0",

  "background": {
      "persistent": true,
      "scripts": [ "background.js" ]
  },
  "permissions": [
    "activeTab",
    "*://*/*", "webRequest", "tabs", "storage"
  ]
}
